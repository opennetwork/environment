{
  "type": "module",
  "name": "@opennetwork/environment",
  "version": "1.18.0",
  "description": "Cross platform JavaScript environment",
  "main": "./esnext/index.js",
  "types": "./esnext/index.d.ts",
  "exports": {
    ".": "./esnext/index.js",
    "./esnext/runtime/run.js": "./esnext/runtime/run.js"
  },
  "author": "Fabian Cook <hello@fabiancook.dev>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jsdom": "^16.2.13",
    "@types/node": "^13.13.4",
    "commander": "^5.1.0",
    "esm": "^3.2.25",
    "filehound": "^1.17.4",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "build": "rm -rf esnext && mkdir esnext && tsc && npm run postbuild",
    "postbuild": "node scripts/post-build.js",
    "deno:dev": "yarn build && yarn deno:example",
    "deno:dev:reload": "yarn build && yarn deno:example:reload",
    "node:dev": "yarn build && yarn node:example",
    "node:example": "FETCH_SERVICE_PORT=8080 node --trace-warnings ./esnext/example/index.js",
    "deno:example": "deno run --allow-net=:8080,:8081,cdn.skypack.dev,deno.land --import-map=./import-map.json --location=\"https://fetch.spec.whatwg.org\" --allow-read --allow-env=FETCH_SERVICE_PORT ./esnext/example/index.js",
    "deno:example:reload": "deno run --allow-net=:8080,:8081,cdn.skypack.dev,deno.land --import-map=./import-map.json --location=\"https://fetch.spec.whatwg.org\" --allow-read --allow-env=FETCH_SERVICE_PORT --reload ./esnext/example/index.js"
  },
  "esm": {
    "cjs": true,
    "mode": "auto",
    "await": true
  },
  "engines": {
    "node": ">=14.1.0"
  },
  "dependencies": {
    "@edwardmx/noop": "^1.0.1",
    "@opennetwork/http-representation": "^3.0.0",
    "@opennetwork/http-representation-node": "^3.0.0",
    "@opennetwork/rdf-data-model": "^5.3.1",
    "@opennetwork/rdf-dataset": "^10.1.4",
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/auto-instrumentations-node": "^0.26.0",
    "@opentelemetry/exporter-jaeger": "^1.0.0",
    "@opentelemetry/exporter-prometheus": "^0.26.0",
    "@opentelemetry/metrics": "^0.24.0",
    "@opentelemetry/sdk-node": "^0.26.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@virtualstate/astro-renderer": "^2.25.1",
    "@virtualstate/dom": "^2.25.1",
    "@virtualstate/examples": "^2.25.1",
    "@virtualstate/fringe": "^2.25.1",
    "@virtualstate/hooks": "^2.25.1",
    "@virtualstate/hooks-extended": "^2.25.1",
    "@virtualstate/union": "^2.25.1",
    "@virtualstate/x": "^2.25.1",
    "abort-controller": "^3.0.0",
    "dependency-graph": "^0.9.0",
    "dom-lite": "^20.2.0",
    "iterable": "^5.7.0",
    "jsdom": "^18.0.1",
    "lit-html": "^1.2.1"
  }
}
