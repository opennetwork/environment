{
  "type": "module",
  "name": "@opennetwork/environment",
  "version": "1.23.0",
  "description": "Cross platform JavaScript environment",
  "main": "./esnext/index.js",
  "types": "./esnext/index.d.ts",
  "exports": {
    ".": "./esnext/index.js",
    "./esnext/runtime/run.js": "./esnext/runtime/run.js",
    "./esnext/fetch/fetch.js": "./esnext/fetch/fetch.js",
    "./fetch": "./esnext/fetch/fetch.js"
  },
  "author": "Fabian Cook <hello@fabiancook.dev>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jsdom": "^16.2.13",
    "@types/node": "^13.13.4",
    "c8": "^7.10.0",
    "commander": "^5.1.0",
    "esm": "^3.2.25",
    "filehound": "^1.17.4",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "build:all": "yarn build:base",
    "build:base": "rm -rf esnext && mkdir esnext && tsc",
    "build:node": "yarn build:base",
    "build:deno": "yarn build:base",
    "build:browser": "yarn build:base",
    "postbuild:all": "yarn postbuild:deno && yarn postbuild:browser",
    "postbuild:base": "node scripts/post-build.js",
    "postbuild:deno": "rm -rf deno && cp -r esnext deno && INPUT=./deno IMPORT_MAP=./import-map-deno.json node scripts/post-build.js",
    "postbuild:browser": "rm -rf browser && cp -r esnext browser && INPUT=./browser IMPORT_MAP=./import-map-browser.json node scripts/post-build.js",
    "deno:dev": "yarn build:deno && FLAGS=POST_CONFIGURE_TEST yarn deno:example",
    "deno:dev:reload": "yarn build:deno && FLAGS=POST_CONFIGURE_TEST yarn deno:example:reload",
    "node:dev": "yarn build:node && FLAGS=POST_CONFIGURE_TEST yarn node:example",
    "node:example": "FETCH_SERVICE_PORT=8080 node --trace-warnings ./esnext/example/index.js",
    "deno:example": "deno run --unstable --allow-net=:8080,:8081,cdn.skypack.dev,deno.land --import-map=./import-map-deno.json --location=\"https://fetch.spec.whatwg.org\" --allow-read --allow-env=FETCH_SERVICE_PORT,FLAGS ./deno/example/index.js",
    "deno:example:reload": "deno run --unstable --allow-net=:8080,:8081,cdn.skypack.dev,deno.land --import-map=./import-map-deno.json --location=\"https://fetch.spec.whatwg.org\" --allow-read --allow-env=FETCH_SERVICE_PORT,FLAGS --reload ./deno/example/index.js",
    "coverage": "yarn build:base && c8 node ./esnext/example/index.js && yarn postbuild:base",
    "clean": "rm -rf esnext && rm -rf deno && rm -rf browser && rm -rf node_modules/.cache && rm -rf node_modules/.store"
  },
  "esm": {
    "cjs": true,
    "mode": "auto",
    "await": true
  },
  "engines": {
    "node": ">=14.1.0"
  },
  "dependencies": {
    "@edwardmx/noop": "^1.0.1",
    "@grpc/grpc-js": "^1.4.3",
    "@opennetwork/environment": "^1.20.1",
    "@opennetwork/http-representation": "^3.0.0",
    "@opennetwork/http-representation-node": "^3.0.0",
    "@opennetwork/rdf-data-model": "^5.3.1",
    "@opennetwork/rdf-dataset": "^10.1.4",
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/auto-instrumentations-node": "^0.26.0",
    "@opentelemetry/exporter-collector-grpc": "^0.25.0",
    "@opentelemetry/exporter-jaeger": "^1.0.0",
    "@opentelemetry/exporter-prometheus": "^0.26.0",
    "@opentelemetry/metrics": "^0.24.0",
    "@opentelemetry/sdk-node": "^0.26.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@types/aws-lambda": "^8.10.85",
    "@virtualstate/astro-renderer": "^2.25.4",
    "@virtualstate/dom": "^2.25.4",
    "@virtualstate/examples": "^2.25.4",
    "@virtualstate/fringe": "^2.25.4",
    "@virtualstate/hooks": "^2.25.4",
    "@virtualstate/hooks-extended": "^2.25.4",
    "@virtualstate/union": "^2.25.4",
    "@virtualstate/x": "^2.25.4",
    "abort-controller": "^3.0.0",
    "dependency-graph": "^0.9.0",
    "dom-lite": "^20.2.0",
    "iterable": "^5.7.0",
    "jsdom": "^18.0.1",
    "lit-html": "^1.2.1",
    "uuid": "^8.3.2"
  }
}
